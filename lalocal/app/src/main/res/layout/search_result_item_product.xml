<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/search_result_item_height"
    android:paddingTop="@dimen/dimen_size_10_dp"
    android:paddingBottom="@dimen/dimen_size_10_dp"
    >
    <include
        layout="@layout/search_result_item"
        />

    <TextView
        android:id="@+id/search_result_item_product_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_size_16_sp"
        android:textColor="@color/color_ffaa2a"
        android:layout_gravity="bottom"
        android:layout_marginStart="@dimen/search_result_item_product_price_left"
        />



</FrameLayout>