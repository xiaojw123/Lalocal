<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:id="@+id/home_me_set"
    android:layout_width="match_parent"
    android:layout_height="125dp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    

    <TextView
        android:id="@+id/item_me_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center_horizontal"
        android:textColor="@color/color_191000"
        android:textSize="@dimen/text_size_12_sp"
        />
    <com.lalocal.lalocal.view.ShapeTextView
        android:id="@+id/item_me_msgcount"
        android:layout_width="22dp"
        android:layout_height="22dp"
        app:corner_radius="@dimen/dimen_size_22_dp"
        app:solid_color="@color/color_ff6f6f"
        android:textColor="@color/white"
        android:textSize="@dimen/text_size_14_sp"
        android:text="0"
        android:layout_gravity="right"
        android:layout_marginRight="@dimen/dimen_size_21_dp"
        android:layout_marginTop="@dimen/dimen_size_15_dp"
        android:gravity="center"
        android:visibility="gone"
        />

</FrameLayout>