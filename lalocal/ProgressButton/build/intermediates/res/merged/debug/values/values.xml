<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="reference" name="progressButtonStyle"/>
    <color name="black_color">#000000</color>
    <color name="white_color">#ffffff</color>
    <declare-styleable name="ProgressButton"><attr name="android:clickable"/><attr name="android:focusable"/><attr format="color" name="defaultBackgroundColor"/><attr format="integer" name="defaultFontSize"/><attr format="color" name="defaultFontColor"/><attr format="string" name="defaultText"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="action_settings">Settings</string>
    <string name="app_name">ProgressButton</string>
    <string name="hello_world">Hello world!</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.



        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="ProgressButton"/>
    <style name="ProgressButton.attrs">
        <item name="defaultBackgroundColor">@color/white_color</item>
        <item name="android:clickable">false</item>
        <item name="android:focusable">false</item>
        <item name="defaultFontSize">40</item>
        <item name="defaultFontColor">@color/black_color</item>
        <item name="defaultText">Load more</item>
    </style>
</resources>